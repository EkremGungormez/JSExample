@{
    ViewBag.Title = "Index";
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <h2>Üye Bilgileri</h2>
     
    <script src="../../Scripts/jquery-1.5.1.min.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-ui-1.8.11.js" type="text/javascript"></script>   
</head>
<body>  
    @using (Html.BeginForm())
    {
        <div>   
            <table>
                <tr>
                    <td>Ad</td><td>@Html.TextBox("Name")</td>
                </tr>
                <tr>
                    <td>Doğum Tarihi</td><td>@Html.TextBox("DatePicker")</td>
                </tr>
                <tr>
                    <td>Email</td><td>@Html.TextBox("Email")</td>
                </tr>
                <tr>
                    <td>Meslek</td><td>@Html.TextBox("Proficiency")</td>
                </tr>
                <tr>
                    <td>Cinsiyet</td><td>@Html.RadioButton("Gender","Erkek",false)Erkek  @Html.RadioButton("Gender","Kadın",false)Kadın</td>
                </tr>
                <tr>
                    <td></td><td><input type="submit" name="Submit" value="Kaydet" onclick="test()" /></td>
                </tr>
            </table>
            
        </div> 
    }
</body>
</html>
<script type="text/javascript">
    $("#DatePicker").datepicker();
    
    try {
    } 
    catch (e) {
    }
    function test() {
        var tarih = new Date();
        var bugun = new Date();
        var tarih = $("#DatePicker").datepicker("getDate");
        if (bugun.getFullYear() - tarih.getFullYear() < 18)
            alert("18 den kucuksun");
        var tari = new Date(Date.parse(($("#DatePicker").val()))); alert(tari.getFullYear())
    }
</script>

